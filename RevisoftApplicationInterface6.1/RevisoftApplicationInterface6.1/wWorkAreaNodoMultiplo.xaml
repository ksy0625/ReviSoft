<Window x:Class="RevisoftApplication.WindowWorkAreaNodoMultiplo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:uc="clr-namespace:UserControls"
        Title="Revisoft Work Area - Tabella Replicata" 
        Height="Auto" Width="680" 
        WindowStartupLocation="Manual"
        Closing="Window_Closing"
        SizeChanged="Window_SizeChanged">
    <ScrollViewer >
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <!--<uc:ucNodoMultiplo Grid.Column="0" x:Name="NodoMultiplo"></uc:ucNodoMultiplo>-->
            <uc:ucNodoMultiploVerticale Grid.Column="0" x:Name="NodoMultiplo" ></uc:ucNodoMultiploVerticale>
        </Grid>    
    </ScrollViewer>
</Window>
