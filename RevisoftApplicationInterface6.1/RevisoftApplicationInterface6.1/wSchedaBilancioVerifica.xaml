<Window x:Class="RevisoftApplication.wSchedaBilancioVerifica"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"         
        Title="Revisoft - Bilancio di Verifica"  
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d" 
        Background="WhiteSmoke" WindowStyle="ToolWindow"        
        SizeToContent="WidthAndHeight"
        >
    <Window.Resources>
        <Style TargetType="Image">
            <Setter Property="Margin" Value="0,0,0,0"/>
        </Style>
    </Window.Resources>
    <StackPanel Background="WhiteSmoke" Height="Auto" HorizontalAlignment="Left" Name="stackPanel1" VerticalAlignment="Top" Width="Auto" Margin="5,0,0,-8">
        <Label Content="Importazione da Bilancio di Verifica" FontSize="16" FontWeight="Bold" Name="labelTitolo" Foreground="#FFF5A41C" Margin="0,0,0,0" Grid.ColumnSpan="2" />
    </StackPanel>
</Window>
