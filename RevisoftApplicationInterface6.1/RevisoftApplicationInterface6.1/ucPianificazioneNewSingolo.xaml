<UserControl x:Class="UserControls.ucPianificazioneNewSingolo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ConvNS"  
             xmlns:uc="clr-namespace:UserControls"
             mc:Ignorable="d"
             SizeChanged="UserControl_SizeChanged">
    <UserControl.Resources>
        <Style TargetType="Image">
            <Setter Property="Margin" Value="0,0,0,0"/>
        </Style>
    </UserControl.Resources>
    <StackPanel x:Name="stack" CanVerticallyScroll="True">       
        <StackPanel Name="brdDefinizione" Grid.IsSharedSizeScope="True" Orientation="Vertical"></StackPanel>
    </StackPanel>
</UserControl>
